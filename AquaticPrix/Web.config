  </system.web>
  <appSettings>
    <add key="URI_CONTACTO" value="http://localhost:29609/api/v1/Contacto" />
    <add key="URI_PERSONAS" value="http://localhost:31781/api/Usuario" />
    <add key="URI_VERIFICARUSUARIO" value="http://localhost:31781/api/Usuario/existe" />
    <add key="URI_VERIFICARCUENTA" value="http://localhost:31781/api/Mail/existe" />
    <add key="URI_LOGINUSUARIO" value="http://localhost:19294/api/Login" />
    <add key="URI_USUARIOCAMBIOCLAVE" value="http://localhost:19294/api/Login" />
    
    <!--Falta el listado y estadisticas-->
    <add key="URI_LISTADOPERSONA" value="http://localhost:47261/api/Persona/perfil/{codPerfil}/opcion/{op}" />
    <add key="URI_USUARIOESTADISTICAS" value="http://localhost:47261/api/Persona/estadisticas" />
